<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- Gray shadow. -->
    <item android:left="10dp" android:top="10dp">
        <shape android:shape="rectangle">
            <gradient android:startColor="#696969"
                android:endColor="#c5c5c5"
                android:centerColor="#696969"
                android:type="radial"/>
            <corners
                android:radius="30dp"/>
        </shape>

    </item>
    <item
        android:drawable="@drawable/rect_button"
       android:bottom="3dp"
        android:left="3dp"
        android:top="3dp"
        android:right="3dp"/>
</layer-list>